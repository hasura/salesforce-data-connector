---
kind: AuthConfig
version: v3
definition:
  mode:
    jwt:
      claimsConfig:
        namespace:
          claimsFormat: Json
          location: /claims.jwt.hasura.io
      tokenLocation:
        type: Header
        name: hasura-user-details
      key:
        fixed:
          algorithm: RS256
          key:
            valueFromEnv: JWT_PUBLIC_KEY

